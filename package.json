{
  "name": "@google-cloud/profiler",
  "version": "0.1.0",
  "description": "Adds support for Google Cloud Profiler to node.js applications",
  "repository": "GoogleCloudPlatform/cloud-profiler-nodejs",
  "main": "compiled-ts/src/index.js",
  "types": "compiled-ts/src/index.d.ts",
  "scripts": {
    "install": "node-gyp rebuild",
    "test": "nyc mocha compiled-ts/test/test-*.js",
    "check": "gts check",
    "clean": "gts clean",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "proto": "npm run proto:profile && npm run proto:profiler",
    "proto:profile": "mkdir -p proto && pbjs -t static-module -w commonjs -o proto/profile.js third_party/proto/profile.proto && pbts -o proto/profile.d.ts proto/profile.js",
    "proto:profiler": "mkdir -p proto && pbjs -t static-module -w commonjs -o proto/profiler.js third_party/googleapis/google/devtools/cloudprofiler/v2/profiler.proto && pbts -o proto/profiler.d.ts proto/profiler.js"
  },
  "author": {
    "name": "Google Inc."
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/common": "^0.14.0",
    "@types/pify": "^3.0.0",
    "bindings": "^1.2.1",
    "delay": "^2.0.0",
    "extend": "^3.0.1",
    "gcp-metadata": "^0.4.0",
    "nan": "^2.1.0",
    "pify": "^3.0.0",
    "request": "^2.83.0",
    "retry-request": "^3.0.1"
  },
  "devDependencies": {
    "@types/delay": "^2.0.0",
    "@types/extend": "^3.0.0",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.30",
    "@types/request": "^2.0.4",
    "@types/sinon": "^2.3.6",
    "clang-format": "^1.0.33",
    "codecov": "^3.0.0",
    "gts": "^0.5.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "protobufjs": "~6.8.0",
    "sinon": "^4.0.1",
    "typescript": "~2.6.x"
  },
  "files": [
    "build/src",
    "bindings",
    "proto",
    "binding.gyp",
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "nyc": {
    "exclude": [
      "proto",
      "build/test"
    ]
  }
}
